{
  "name": "congregreat-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start-dev": "DB_HOSTNAME=${DB_HOSTNAME:-localhost} NODE_ENV=${NODE_ENV:-development} nodemon ./bin/www",
    "test": "standard && nyc --reporter=html --reporter=text env JWT_PUBLIC=test/pems/public.pem JWT_PRIVATE=test/pems/private.pem JWT_PASSPHRASE=cgtest NODE_ENV=test mocha --recursive --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "seed": "node_modules/.bin/sequelize db:seed:all",
    "seed-undo": "node_modules/.bin/sequelize db:seed:undo:all"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "handlebars": "^4.0.11",
    "jsonwebtoken": "^8.2.0",
    "morgan": "~1.9.0",
    "mysql2": "^1.5.3",
    "nodemailer": "^4.6.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "sequelize": "^4.37.3",
    "sequelize-cli": "^4.0.0",
    "serve-favicon": "~2.4.5",
    "uuid": "^3.2.1",
    "xlsx": "^0.14.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.0",
    "faker": "^4.1.0",
    "mocha": "^5.0.4",
    "nodemon": "^1.18.3",
    "nyc": "^13.1.0",
    "sinon": "^4.4.8",
    "standard": "^11.0.1"
  }
}
